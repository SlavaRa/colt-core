<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import java.net.*?>
<?import java.util.*?>
<?import javafx.geometry.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.paint.*?>

<AnchorPane prefHeight="400" prefWidth="600" xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/2.2">
<children>
    <StackPane layoutX="145" layoutY="166" minHeight="-Infinity" minWidth="-Infinity" prefHeight="94" prefWidth="206" styleClass="player-box">
    <children>
        <HBox spacing="10">
        <children>
            <ToggleButton contentDisplay="GRAPHIC_ONLY" focusTraversable="false" prefHeight="24" prefWidth="20" styleClass="btn-player-pause" text="Pause">
                <HBox.margin>
                <Insets top="7" />
                </HBox.margin>
            </ToggleButton>
            <ToggleButton contentDisplay="graphic_only" focusTraversable="false" prefHeight="24" prefWidth="20" styleClass="btn-player-play" text="Play">
                <HBox.margin>
                <Insets top="7" />
                </HBox.margin>
            </ToggleButton>
            <ToggleButton contentDisplay="graphic_only" focusTraversable="false" prefHeight="24" prefWidth="20" styleClass="btn-player-stop" text="Stop">
                <HBox.margin>
                <Insets top="7" />
                </HBox.margin>
            </ToggleButton>
            <Separator minHeight="-Infinity" orientation="VERTICAL" prefHeight="38" prefWidth="1" />
            <Button contentDisplay="graphic_only" focusTraversable="false" prefHeight="24" prefWidth="20" styleClass="btn-player-add" text="Add">
                <HBox.margin>
                <Insets top="7" />
                </HBox.margin>
            </Button>
        </children>
        <padding>
            <Insets left="3" bottom="11" top="9" />
        </padding>
        </HBox>
    </children>
    </StackPane>
</children>

    <stylesheets>
        <URL value="@../codeOrchestra/colt/core/ui/style/main.css" />
    </stylesheets>

</AnchorPane>
